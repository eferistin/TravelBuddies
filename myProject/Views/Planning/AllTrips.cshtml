<h2>Hey, @ViewBag.name</h2>

<br />
<h3>Here is a list of your plan trips/activies thus far.</h3>
<br />

@*@if (Model.Count ==0)
{
    <p>There are no plan trips.</p>
}*@
<ol>
    @foreach (var funplans in ViewBag.yourPlans)
    {
        <li>
            <a href="/Planning/FindMatch?TripID=@funplans.ID">
                <ul>
                    <li>@funplans.Activity</li>
                    <li>@funplans.Location</li>
                    <li>@funplans.Date</li>
                </ul>
            </a>
        </li>

    }
</ol>



  